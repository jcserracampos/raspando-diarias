<p id="notice"><%= notice %></p>

<h1>Diaria</h1>

<table>
  <thead>
    <tr>
      <th>Orgao superior</th>
      <th>Orgao</th>
      <th>Unidade gestora</th>
      <th>Favorecido</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diaria.each do |diarium| %>
      <tr>
        <td><%= diarium.orgao_superior %></td>
        <td><%= diarium.orgao %></td>
        <td><%= diarium.unidade_gestora %></td>
        <td><%= diarium.favorecido %></td>
        <td><%= diarium.valor %></td>
        <td><%= link_to 'Show', diarium %></td>
        <td><%= link_to 'Edit', edit_diarium_path(diarium) %></td>
        <td><%= link_to 'Destroy', diarium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diarium', new_diarium_path %>
